{
  "builds": [
    {
      "src": "gutenberg/wsgi.py",
      "use": "@vercel/python",
      "config": { "maxLambdaSize": "15mb", "runtime": "python3.11" }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "gutenberg/wsgi.py"
    }
  ],
  "functions": {
    "app/api/**/*": {
      "maxDuration": 50
    }
  }
}
